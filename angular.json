{
  "version": 1,
  "projects": {
    "app-adapters": {
      "projectType": "application",
      "root": "apps/app-adapters",
      "sourceRoot": "apps/app-adapters/src",
      "prefix": "app-adapters",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/app-adapters",
            "index": "apps/app-adapters/src/index.html",
            "main": "apps/app-adapters/src/main.ts",
            "polyfills": "apps/app-adapters/src/polyfills.ts",
            "tsConfig": "apps/app-adapters/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/app-adapters/src/favicon.ico",
              "apps/app-adapters/src/assets"
            ],
            "styles": ["apps/app-adapters/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/app-adapters/src/environments/environment.ts",
                  "with": "apps/app-adapters/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "app-adapters:build:production"
            },
            "development": {
              "browserTarget": "app-adapters:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-adapters:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/app-adapters/src/**/*.ts",
              "apps/app-adapters/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app-adapters"],
          "options": {
            "jestConfig": "apps/app-adapters/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "flow-adapters": {
      "projectType": "library",
      "root": "libs/flow-adapters",
      "sourceRoot": "libs/flow-adapters/src",
      "prefix": "flow-adapters",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/flow-adapters/tsconfig.lib.json",
            "project": "libs/flow-adapters/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/flow-adapters/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/search"],
          "options": {
            "jestConfig": "libs/flow-adapters/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/flow-adapters/src/**/*.ts",
              "libs/flow-adapters/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "select": {
      "projectType": "library",
      "root": "libs/select",
      "sourceRoot": "libs/select/src",
      "prefix": "flow-adapters",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/select/tsconfig.lib.json",
            "project": "libs/select/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/select/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/select"],
          "options": {
            "jestConfig": "libs/select/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/select/src/**/*.ts",
              "libs/select/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "defaultProject": "app-adapters",
  "schematics": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
